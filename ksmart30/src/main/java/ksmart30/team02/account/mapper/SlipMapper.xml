<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ksmart30.team02.account.mapper.SlipMapper">
	<!-- 전표리스트 SELECT 쿼리문 -->
	<select id="SlipList"
			resultType="ksmart30.team02.account.domain.Slip">
		SELECT 
			SLIP_SEQ,
			ACC_CD,
			ACC_DTL_CD,
			BELONG_DEPT_CD,
			DRCR_GB,
			ACC_AMT			
		FROM
			T_AC1120
		
	</select>
</mapper>